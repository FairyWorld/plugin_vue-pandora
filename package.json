{
  "name": "vue-pandora-next-v2",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "jest",
    "cz": "npx git-cz"
  },
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts?(x),json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "element-plus": "^1.0.1-alpha.14",
    "vue": "^3.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "@vue/compiler-sfc": "^3.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.13.0",
    "eslint-plugin-vue": "^7.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5",
    "vite": "^1.0.0-rc.9"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
