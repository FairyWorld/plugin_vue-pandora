{
  "name": "vue-pandora-next-v2",
  "private": true,
  "scripts": {
    "build": "esno ./scripts/build.ts",
    "build:types": "tsc --emitDeclarationOnly && npm run types:fix",
    "test": "jest",
    "lint": "eslint './{packages,.storybook,scripts}/**/*.{js,ts,tsx,vue,md}'",
    "lint:fix": "npm run lint -- --fix",
    "cz": "npx git-cz",
    "sb": "start-storybook -p 6006 ",
    "sb:build": "build-storybook",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "gen-form-comp": "bash ./scripts/gc-form-comp.sh",
    "clear": "rimraf node_modules storybook-static "
  },
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{packages,.storybook,scripts}/**/*.{js,ts,tsx,vue,md} *.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
